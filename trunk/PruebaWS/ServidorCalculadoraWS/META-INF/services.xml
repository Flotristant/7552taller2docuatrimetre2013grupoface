<service>
	<description> Web service que funciona como una calculadora
	</description>
	<messageReceivers>
		<messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-out"
			class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
	</messageReceivers>
	<parameter name="ServiceClass">com.ws.servidor.Calculadora 
	</parameter>
	<operation name="suma" mep="http://www.w3.org/2004/08/wsdl/in-out" />
	<operation name="resta" mep="http://www.w3.org/2004/08/wsdl/in-out" />
	<operation name="multiplica" mep="http://www.w3.org/2004/08/wsdl/in-out" />
	<operation name="divide" mep="http://www.w3.org/2004/08/wsdl/in-out" />
	
</service>